{
  "name": "operator-uplift",
  "version": "1.0.0",
  "description": "AI-Powered Self-Progression Platform - Transform your goals into epic quests with gamified productivity",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000 --open=/",
    "dev": "live-server --port=3000 --open=/",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint assets/js/**/*.js",
    "lint:fix": "eslint assets/js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "deploy": "netlify deploy --prod",
    "deploy:preview": "netlify deploy",
    "analyze": "webpack-bundle-analyzer build/js/stats.json",
    "security:audit": "npm audit",
    "performance:audit": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "clean": "rimraf build dist",
    "serve": "http-server build -p 8080 -o"
  },
  "keywords": [
    "productivity",
    "goal-setting",
    "ai",
    "gamification",
    "self-improvement",
    "pwa",
    "firebase",
    "netlify"
  ],
  "author": "Operator Uplift Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/operatoruplift/operator-uplift.git"
  },
  "bugs": {
    "url": "https://github.com/operatoruplift/operator-uplift/issues"
  },
  "homepage": "https://operatoruplift.com",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "firebase": "^10.7.0",
    "firebase-admin": "^13.4.0",
    "gsap": "^3.12.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.0",
    "tone": "^14.7.77",
    "tsparticles": "^2.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "lighthouse": "^11.4.0",
    "live-server": "^1.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "pwa": {
    "name": "Operator Uplift",
    "short_name": "Operator Uplift",
    "description": "AI-Powered Self-Progression Platform",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#0a0a0a",
    "theme_color": "#f97316",
    "icons": [
      {
        "src": "/android-chrome-192x192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "/android-chrome-512x512.png",
        "sizes": "512x512",
        "type": "image/png"
      }
    ]
  }
}
